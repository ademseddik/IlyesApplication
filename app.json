{
  "expo": {
    "name": "Tawasalna",
    "slug": "Tawasalna",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/TawasalnaLogoW1.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "androidStatusBar": {
      "backgroundColor": "#00000000",
      "translucent": true
    },
    "androidNavigationBar": {
      "backgroundColor": "#2A003A"
    },
    "ios": {
      "newArchEnabled": true,
      "supportsTablet": true,
      "splash": {
        "image": "./assets/images/TawasalnaLogoW1.png",
        "resizeMode": "cover",
        "tabletImage": "./assets/images/splash-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "android": {
      "newArchEnabled": true,
      "package": "com.ademseddik.test",
      "splash": {
        "resizeMode": "contain",
        "image": "./assets/images/TawasalnaLogoW1.png",
        "backgroundColor": "#4e2eb0"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
